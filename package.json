{
  "name": "notely",
  "productName": "notely",
  "version": "0.1.0",
  "private": true,
  "author": "Jes",
  "description": "A simple text editor that allows basic formatting and storing files locally.",
  "dependencies": {
    "@editorjs/editorjs": "^2.29.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tiptap/core": "^2.2.1",
    "@tiptap/extension-character-count": "^2.2.1",
    "@tiptap/extension-placeholder": "^2.2.3",
    "@tiptap/extension-text-align": "^2.2.1",
    "@tiptap/extension-underline": "^2.1.16",
    "@tiptap/pm": "^2.1.16",
    "@tiptap/react": "^2.2.3",
    "@tiptap/starter-kit": "^2.1.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "fs": "^0.0.1-security",
    "install": "^0.13.0",
    "npm": "^10.4.0",
    "os": "^0.1.2",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.32.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-modal": "^3.16.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "5.0.1",
    "tippy.js": "^6.3.7",
    "tiptap": "^1.32.2",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.21"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "note": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "note:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "extends": null,
    "appId": "",
    "mac": {
      "icon": "",
      "target": [
        "dmg"
      ]
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "electron-is-dev": "^3.0.1"
  }
}
